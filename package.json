{
  "name": "britecharts",
  "version": "1.0.15",
  "description": "Eventbrite's Charting Library",
  "main": "README.md",
  "scripts": {
    "dev": "WEBPACK_ENV=dev ./node_modules/.bin/webpack",
    "prod": "WEBPACK_ENV=prod ./node_modules/.bin/webpack",
    "prodUMD": "WEBPACK_ENV=prodUMD ./node_modules/.bin/webpack",

    "build": "npm run prod && npm run prodUMD",
    "buildAll": "npm run dev && npm run prod && npm run prodUMD",

    "test": "WEBPACK_ENV=test karma start",
    "docs": "npm run dev && grunt docs",
    "release": "npm run buildAll && grunt release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eventbrite/britecharts.git"
  },
  "keywords": [
    "d3",
    "chart",
    "graph"
  ],
  "publishConfig": {
    "registry": "http://packages:8889"
  },
  "author": "Eventbrite",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eventbrite/britecharts/issues"
  },
  "homepage": "https://github.com/eventbrite/britecharts",
  "dependencies": {
    "d3": "^3.5.5"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-istanbul": "^0.6.0",
    "babel-istanbul-loader": "0.0.3",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "generator-ebjs": "git+ssh://git@github.com/eventbrite/generator-ebjs.git",
    "grunt": "~0.4.5",
    "grunt-babel": "^6.0.0",
    "grunt-contrib-connect": "^0.11.2",
    "grunt-contrib-requirejs": "~0.4.4",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-jsdoc": "^0.6.8",
    "grunt-karma": "^0.12.1",
    "grunt-release": "~0.9.0",
    "grunt-shell": "~1.1.2",
    "ink-docstrap": "~0.4.12",
    "jasmine-jquery": "^2.0.6",
    "jquery": "^2.1.4",
    "jsdoc": "~3.2.2",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "~0.1.10",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "~0.3.5",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "load-grunt-tasks": "~0.4.0",
    "reggie": "~0.2.1",
    "shelljs": "~0.3.0",
    "sinon": "~1.9.0",
    "underscore": "~1.8.3",
    "webpack": "^1.12.13"
  }
}
