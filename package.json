{
  "name": "britecharts",
  "version": "2.6.0",
  "description": "D3.js based Modular Charting Library by Eventbrite",
  "main": "dist/bundled/britecharts.min.js",
  "contributors": [
    {
      "name": "Marcos Iglesias",
      "email": "golodhros@gmail.com"
    },
    {
      "name": "Ryan Wholey",
      "email": "rjwholey@gmail.com"
