{
  "name": "britecharts",
  "version": "1.1.11",
  "description": "Eventbrite's Charting Library",
  "main": "README.md",
  "scripts": {
    "demos:compile": "WEBPACK_ENV=demos webpack --progress --colors",
    "demos:watch": "WEBPACK_ENV=demos webpack --watch --progress --colors",
    "demos:serve": "WEBPACK_ENV=demos webpack-dev-server --inline --hot",
    "styles": "grunt lint && grunt sass",
    "docs": "WEBPACK_ENV=demos webpack && npm run styles && grunt docs",
    "prod": "npm run _prod && npm run _prodUMD",
    "untrack": "grunt untrack",
    "release": "npm run prod && grunt release",
    "test": "WEBPACK_ENV=test karma start",
    "_prod": "WEBPACK_ENV=prod webpack",
    "_prodUMD": "WEBPACK_ENV=prodUMD webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eventbrite/britecharts.git"
  },
  "keywords": [
    "d3",
    "chart",
    "graph"
  ],
  "publishConfig": {
    "registry": "http://packages:8889"
  },
  "author": "Eventbrite",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/eventbrite/britecharts/issues"
  },
  "homepage": "https://github.com/eventbrite/britecharts",
  "dependencies": {
    "base-64": "^0.1.0",
    "bowser": "^1.5.0",
    "d3": "^4.4.0",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.6.0",
    "babel-istanbul-loader": "0.0.3",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.18.0",
    "generator-ebjs": "git+ssh://git@github.com/eventbrite/generator-ebjs.git",
    "grunt": "~0.4.5",
    "grunt-babel": "^6.0.0",
    "grunt-contrib-connect": "^0.11.2",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-requirejs": "~0.4.4",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-jsdoc": "^0.6.8",
    "grunt-karma": "^0.12.1",
    "grunt-release": "~0.9.0",
    "grunt-reload": "^0.2.0",
    "grunt-sass": "^1.2.1",
    "grunt-sass-lint": "^0.2.0",
    "grunt-shell": "~1.1.2",
    "ink-docstrap": "~0.4.12",
    "jasmine-core": "^2.4.1",
    "jasmine-jquery": "^2.0.6",
    "jquery": "^3.1.1",
    "jsdoc": "^3.4.3",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "~0.1.10",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "~0.3.5",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "load-grunt-tasks": "~0.4.0",
    "reggie": "~0.2.1",
    "requirejs-plugins": "^1.0.2",
    "shelljs": "~0.3.0",
    "sinon": "^1.17.6",
    "text": "requirejs/text",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-livereload-plugin": "^0.7.0"
  }
}
