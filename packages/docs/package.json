{
  "name": "@britecharts/docs",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn run docs:readme && yarn run docs:api && docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "docs:readme": "cp ../../README.md ./docs/.",
    "docs:readmeNotWorking": "echo -e \"--\nid: README\nslug: /\n---\n$ (cat README.md)\" > packages/docs/docs/README.md",
    "docs:api": "yarn run jsdoc2md -f ../core/src/charts/**/*.js -c jsdoc.conf.json  > ./docs/API.md"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.17",
    "@docusaurus/preset-classic": "2.0.0-beta.17",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "docusaurus-lunr-search": "^2.1.15",
    "jsdoc": "3.6.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^7.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
