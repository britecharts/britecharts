{
  "name": "@britecharts/docs",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.0",
  "dependencies": {
    "@docusaurus/core": "2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "docusaurus-lunr-search": "^2.1.15",
    "jsdoc": "3.6.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^7.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "docusaurus build",
    "clean": "yarn run clear",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docs:api": "node ./scripts/generate-docs.js",
    "docs:readme": "node ./scripts/copy-readme.js",
    "docusaurus": "docusaurus",
    "prebuild": "yarn run clean",
    "serve": "docusaurus serve",
    "start": "yarn run docs:readme && yarn run docs:api && docusaurus start",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  }
}
