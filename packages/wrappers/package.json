{
  "name": "@britecharts/wrappers",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.0",
  "main": "dist/umd/bundle/wrappers.bundled.min.js",
  "module": "src/index.js",
  "dependencies": {
    "@britecharts/core": "workspace:^",
    "d3-selection": "^1.4.1"
  },
  "devDependencies": {
    "webpack": "4.46.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.7.2",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-merge": "4.2.1"
  },
  "browserslist": "> 0.25%, not dead",
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-proposal-function-bind",
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "scripts": {
    "test": "jest --colors",
    "test:watch": "jest --watch --colors",
    "lint:js": "eslint src/charts/ src/helpers webpack.*.js --cache",
    "lint": "yarn run lint:js",
    "prebuild": "rimraf dist",
    "build": "webpack --env=production",
    "build:bundle": "webpack --env=prodBundleConfig",
    "build:charts": "webpack --env=prodChartsConfig"
  }
}
