{
    "tags": {
        "allowUnknownTags": true
    },
    "ops": {
        "tutorials": "./tutorials",
        "demos": "./demos"
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": ["./README.md"]
    },
    "plugins": [],
    "templates": {
        "logoFile": "/src/doc/template/static/img/logo.svg",
        "systemName": "Britecharts",
        "footer": "",
        "copyright": "Eventbrite Copyright",
        "includeDate": false,
        "navType": "inline",
        "theme": "flatly",
        "linenums": false,
        "collapseSymbols": false,
        "inverseNav": false,
        "cleverLinks": false,
        "outputSourceFiles": true,
        "syntaxTheme": "default",
        "monospaceLinks": false,
        "search": true,

        "googleAnalytics": "",
        "openGraph": {
            "title": "TestOpenGraph",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "Britecharts - D3.js based charting library of reusable components",
            "description": "Britecharts is a client-side reusable Charting Library based on D3.js v4 that allows easy and intuitive use of charts and components that can be composed together creating amazing visualizations.",
            "keyword": "Britecharts, D3.js"
        }
    }
}
