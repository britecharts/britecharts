{
    "tags": {
        "allowUnknownTags": true
    },
    "ops": {
        "tutorials": "./tutorials",
        "demos": "./demos"
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": ["./README.md"]
    },
    "plugins": [],
    "templates": {
        "systemName": "Britecharts",
        "footer": "",
        "copyright": "Eventbrite Copyright",
        "includeDate": false,
        "navType": "inline",
        "theme": "flatly",
        "linenums": false,
        "collapseSymbols": false,
        "inverseNav": false,
        "cleverLinks": false,
        "outputSourceFiles": true,
        "syntaxTheme": "default",
        "monospaceLinks": false,
        "search": true
    }
}
